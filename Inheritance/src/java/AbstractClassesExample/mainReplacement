package com;


import AbstractClassesExample.Banana;
import AbstractClassesExample.Fruit;
import AbstractClassesExample.Mango;

public class Main {

    public static void main(String[] args) {

        Fruit mango = new Mango();
        Fruit banana = new Banana();

        mango.SetSeason("Summer");
        mango.SetName("Chonsa Mango");

        banana.GetCompanyName();
        mango.GetCompanyName();

        System.out.println("name: " + mango.GetName());
        System.out.println("season: " + mango.GetSeason());

        mango.CalculatePrice();
        banana.CalculatePrice();
    }
}
